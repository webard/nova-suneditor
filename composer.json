{
    "name": "webard/nova-suneditor",
    "description": "Custom field for Laravel Nova that integrates the SunEditor, a lightweight and flexible WYSIWYG editor",
    "keywords": [
        "laravel",
        "nova",
        "suneditor",
        "wysiwyg"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.20|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Webard\\NovaSunEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webard\\NovaSunEditor\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.0"
    }
}
